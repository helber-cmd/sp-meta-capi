generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model LeadContext {
  id        String   @id @default(uuid())
  lead_id   String   @unique
  afp       String?  

  // cookies/ids
  fbp       String?
  fbc       String?
  fbclid    String?

  // utms
  utm_source   String?
  utm_medium   String?
  utm_campaign String?
  utm_content  String?

  // device
  client_ip_address   String?
  client_user_agent   String?

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([afp])
  @@index([createdAt])
}


